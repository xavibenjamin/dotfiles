# Local/private config goes in the include

[commit]
  gpgsign = true

[include]
  path = ~/.gitconfig.local

[color]
  diff = auto
  status = auto
  branch = auto
  ui = true

[core]
  excludesfile = ~/.gitignore
  editor = vim

[init]
  templatedir = ~/.git_template

[gpg]
  program = gpg2
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
